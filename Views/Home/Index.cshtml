@{
    ViewBag.Title = "Home Page";
}


<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>


<script src="https://code.getmdl.io/1.3.0/material.min.js"></script>
<link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
<!-- Material Design icon font -->
<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

<p></p>

<div ng-app="myApp" ng-controller="myCtrl">
    <div ng-controller="angularController">
        <h1>Seleted ITEM</h1>
        <table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp">
            <thead style="border-color:dodgerblue">
                <tr>
                    <th class="mdl-data-table__cell--non-numeric">コード</th>
                    <th>図番番号</th>
                    <th>名称</th>
                    <th>機種</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="sehin in selectedSehins">
                    <td>
                        <button class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-button--colored" ng-click="del(sehin)">
                            <i class="material-icons" >remove</i>
                        </button>
                    </td>
                    <td class="mdl-data-table__cell--non-numeric">{{sehin.Code}} </td>
                    <td>{{sehin.Zuban}}</td>
                    <td>{{sehin.Name}}</td>
                    <td>{{sehin.Kisyu}}</td>
                </tr>
            </tbody>
        </table>

   
        
        <button class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored" ng-click="doPost123()" ng-confirm-click="Would you like to say hi?">
            Submit
        </button>
        <p></p>
        <div class="mdl-textfield mdl-js-textfield mdl-textfield--expandable">
            <label class="mdl-button mdl-js-button mdl-button--icon" for="sample6">
                <i class="material-icons">search</i>
            </label>
            <div class="mdl-textfield__expandable-holder">
                <input class="mdl-textfield__input" type="text" id="sample6" ng-model="test">
                <label class="mdl-textfield__label" for="sample-expandable">Expandable Input</label>
            </div>
        </div>

        <table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp">
            <thead>
                <tr>
                    <!-- <td>CheckBox</td> -->
                    <th class="mdl-data-table__cell--non-numeric">コード</th>
                    <th>図番番号</th>
                    <th>名称</th>
                    <th>機種</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="sehin in listSehin | filter:test | orderBy:'Code'">
                    <!-- <td><input type="checkbox" ng-click="clickedItem(sehin)" </td> -->
                    <td>
                        <button class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab" ng-click="clickedItem(sehin)">
                            <i class="material-icons" >add</i>
                        </button>
                    </td>
                    <td>{{sehin.Code}}</td>
                    <td><span ng-dblclick="doubleClickedItem(sehin)">{{sehin.Zuban}}</span></td>
                    <td><span ng-dblclick="doubleClickedItem(sehin)">{{sehin.Name}}</span></td>
                    <td>{{sehin.Kisyu}}</td>
                </tr>
            </tbody>
        </table>
    </div>



</div>
<script src="~/TestJS/myApp.js"></script>
<script src="~/TestJS/myCtrl.js"></script>


    


    




